def nextprime(n):

    if n < 2:
        print("2")
        exit()

    while True:
        n += 1
        for i in range(2,n):
            if n % i == 0:
                break

        else:
            print(n)
            break


try:
    x = int(input("enter a number:"))
    nextprime(x)


except ValueError:
    print("Enter an integer")
